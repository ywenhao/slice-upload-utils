{
  "name": "slice-upload-utils",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@8.1.1",
  "description": "A library for slice upload file and download.",
  "author": "Ywenhao <actions.win@foxmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/ywenhao",
  "homepage": "https://github.com/antfu/slice-upload-utils#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ywenhao/slice-upload-utils.git"
  },
  "bugs": "https://github.com/ywenhao/slice-upload-utils/issues",
  "keywords": [
    "file",
    "slice",
    "upload",
    "分片上传",
    "大文件",
    "big-file",
    "slice-upload",
    "文件上传",
    "文件下载",
    "file-chunk"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "pnpm -C playground/vue dev",
    "stub": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "pnpm build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "axios": "^1.3.5",
    "spark-md5": "^3.0.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.4",
    "@types/spark-md5": "^3.0.2",
    "bumpp": "^9.1.0",
    "eslint": "^8.37.0",
    "esno": "^0.16.3",
    "lint-staged": "^13.2.0",
    "pnpm": "^8.1.1",
    "rimraf": "^4.4.1",
    "simple-git-hooks": "^2.8.1",
    "typescript": "^5.0.3",
    "unbuild": "^1.2.0",
    "vite": "^4.2.1",
    "vitest": "^0.29.8"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
